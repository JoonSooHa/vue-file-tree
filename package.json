    "@fortawesome/free-solid-svg-icons": "^5.2.0",
    "@fortawesome/vue-fontawesome": "^0.1.1",
    "mime": "2.3.1",
    "path-directories": "^1.0.1",
    "sl-vue-tree": "^1.7.0",
    "style-loader": "^0.21.0",
    "walkdir": "0.0.12"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^4.5.1",
